{
  "name": "react-cnode",
  "version": "1.0.0",
  "description": "react cnode webapp",
  "main": "index.js",
  "scripts": {
    "build:client": "webpack --config build/webpack.prod.conf.js",
    "build:server": "webpack --config build/webpack.server.conf.js",
    "dev:client": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "clear": "rimraf dist",
    "build": "npm run clear && npm run build:client && npm run build:server",
    "start": "node server/server.js"
  },
  "keywords": [
    "react",
    "ssr"
  ],
  "author": "liuyao",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "express": "^4.16.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-hot-loader": "^4.3.10"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.2",
    "html-webpack-plugin": "^3.2.0",
    "rimraf": "^2.6.2",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8",
    "webpack-merge": "^4.1.4"
  }
}
