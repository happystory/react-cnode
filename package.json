{
  "name": "react-cnode",
  "version": "1.0.0",
  "description": "react cnode webapp",
  "main": "index.js",
  "scripts": {
    "build:client": "webpack --config build/webpack.prod.conf.js",
    "build:server": "webpack --config build/webpack.server.conf.js",
    "dev:client": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "dev:server": "nodemon server/server.js",
    "clear": "rimraf dist",
    "build": "npm run clear && npm run build:client && npm run build:server",
    "lint": "eslint --ext .js src",
    "precommit": "npm run lint"
  },
  "keywords": [
    "react",
    "ssr"
  ],
  "author": "liuyao",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "http-proxy-middleware": "^0.19.0",
    "memory-fs": "^0.4.1",
    "mobx": "^5.1.2",
    "mobx-react": "^5.2.8",
    "nodemon": "^1.18.4",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-hot-loader": "^4.3.10",
    "react-router-dom": "^4.3.1",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "rimraf": "^2.6.2",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8",
    "webpack-merge": "^4.1.4"
  }
